    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/unzipper": "^0.10.5",
    "json-schema-to-ts": "^3.0.0",
    "serverless-lift": "^1.29.0",
