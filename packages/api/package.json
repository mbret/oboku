{
  "name": "@oboku/api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "tsc": "tsc --noEmit",
    "api:prepare-layer": "npx ts-node scripts/prepareLayers.ts",
    "api:start": "sls offline --httpPort 3001",
    "api:package": "serverless package",
    "build": "tsc --noEmit",
    "postinstall": "rm -rf node_modules/serverless"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "node-unrar-js": "^2.0.2",
