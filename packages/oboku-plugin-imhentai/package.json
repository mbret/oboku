{
  "name": "@oboku/plugin-imhentai",
  "version": "0.0.30",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "concurrently -n front,back \"yarn start:front\" \"yarn start:back\"",
    "start:front": "concurrently -n src,types \"yarn start-src:front\" \"yarn start-types:front\"",
    "start:back": "concurrently -n src,types \"yarn start-src:back\" \"yarn start-types:back\"",
    "start-src:front": "webpack build --config ./webpack.config.front.js --watch",
    "start-types:front": "tsc -w --project tsconfig.front.json --preserveWatchOutput --declaration --noEmit false --emitDeclarationOnly",
    "start-src:back": "webpack build --config ./webpack.config.back.js --watch",
    "start-types:back": "tsc -w --project tsconfig.back.json --preserveWatchOutput --declaration --noEmit false --emitDeclarationOnly",
    "build-src:front": "webpack build --config ./webpack.config.front.js",
    "build-types:front": "tsc --project tsconfig.front.json --declaration --noEmit false --emitDeclarationOnly",
    "build-src:back": "webpack build --config ./webpack.config.back.js",
    "build-types:back": "tsc --project tsconfig.back.json --declaration --noEmit false --emitDeclarationOnly",
    "build:front": "concurrently -n src,types \"yarn build-src:front\" \"yarn build-types:front\"",
    "build:back": "concurrently -n src,types \"yarn build-src:back\" \"yarn build-types:back\"",
    "build": "concurrently -n front,back \"yarn build:front\" \"yarn build:back\"",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "tsc": "concurrently -n front,back \"yarn tsc:front\" \"yarn tsc:back\"",
    "tsc:front": "tsc --project tsconfig.front.json",
    "tsc:back": "tsc --project tsconfig.back.json",
    "test": "jest"
  },
  "peerDependencies": {
    "@oboku/plugin-back": "0.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "26.6.0",
    "ts-loader": "^8.2.0",
    "typescript": "*",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.9.1"
  }
}
