{
  "name": "@oboku/plugin-back",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "concurrently -n src,types \"yarn start-src\" \"yarn start-types\"",
    "start-src": "webpack build --config ./webpack.config.js --watch",
    "build-src": "webpack build --config ./webpack.config.js",
    "start-types": "yarn tsc -w --preserveWatchOutput --declaration --noEmit false --emitDeclarationOnly",
    "build-types": "yarn tsc --declaration --noEmit false --emitDeclarationOnly",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "26.6.0",
    "ts-loader": "^8.2.0",
    "typescript": "*",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.9.1"
  }
}
